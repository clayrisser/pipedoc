{
  "name": "pipedoc-example",
  "version": "0.0.9",
  "description": "pipedoc example",
  "keywords": [
    "pipedoc",
    "doc",
    "documentation",
    "pipedoc"
  ],
  "homepage": "https://github.com/codejamninja/pipedoc",
  "bugs": {
    "url": "https://github.com/codejamninja/pipedoc/issues",
    "email": "jam@codejam.ninja"
  },
  "license": "MIT",
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
    "url": "https://codejam.ninja"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/codejamninja/pipedoc"
  },
  "scripts": {
    "build": "make -s build",
    "clean": "make -s clean",
    "format": "make -s format",
    "inc": "npm version patch --git=false 2>/dev/null || true",
    "lint": "make -s lint",
    "make": "make --version",
    "prepare": "make -s prepare",
    "purge": "make -s purge",
    "test": "make -s test",
    "test:watch": "make -s test-watch"
  },
  "dependencies": {
    "@oclif/command": "^1.7.0",
    "@oclif/config": "^1.16.0",
    "@oclif/errors": "^1.3.3",
    "@oclif/parser": "^3.8.5",
    "core-js": "^3.6.5",
    "execa": "^4.0.3",
    "fs-extra": "^9.0.1",
    "globby": "^11.0.1",
    "pipedoc": "workspace:*",
    "pkg-dir": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "cspell": "^4.0.63",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-jam": "^0.2.20",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "gnumake": "^0.3.5",
    "jest": "^26.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "lockfile-lint": "^4.3.7",
    "majestic": "^1.7.0",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintIgnore": [
    "lib"
  ],
  "pipedoc": true,
  "jest": {
    "testRegex": "tests(/|/.*/)[^_/]*[jt]sx?$",
    "verbose": true
  },
  "jestSonar": {
    "reportPath": "node_modules/.tmp/reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
